    {% extends 'base.html.twig' %}
    {% block title %}TipoOperacion{% endblock %}
    {% block breadcrumb %}
    {{ breadcrumb.breadcrumb(['Inicio','NIVEL_2','NIVEL_3']) }}
    {% endblock %}
    {% block title_portlet %}Mostrando TipoOperacion{% endblock %}

    {% block body %}
    <div class="row">
        <div class="col-md-6">
            <table class="table table-striped table-bordered table-hover col-md-offset-4">
                <tbody>
                    <tr>
                        <th>Id</th>
                            <td>{{ tipoOperacion.id }}</td>
                    </tr>
                    <tr>
                        <th>Descripcion</th>
                            <td>{{ tipoOperacion.descripcion }}</td>
                    </tr>
                    <tr>
                        <th>Descuento</th>
                            <td>{{ tipoOperacion.descuento }}</td>
                    </tr>
                </tbody>
            </table>

            <div class="col-md-6 margin-top-20">
                {{ form_start(delete_form) }}
                <a class="btn btn-default" href="{{ path('tipooperacion_index') }}">Volver</a>
                <a class="btn btn-default"
                   href="{{ path('tipooperacion_edit', { 'id': tipoOperacion.id }) }}">Editar</a>
                <input class="btn btn-danger" type="submit" value="Delete">
            </div>
        </div>
    </div>
    {% endblock %}
